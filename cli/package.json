{
  "name": "towles-tool",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.10.0",
  "description": "A common set of CLI tools I use",
  "license": "MIT",
  "homepage": "https://github.com/ChrisTowles/dotfiles#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisTowles/dotfiles.git"
  },
  "bugs": {
    "url": "https://github.com/ChrisTowles/dotfiles/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "towles-cli": "bin/cli.mjs",
    "tt": "bin/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env DEBUG=towles-cli:* esno ./src/cli.ts",
    "start": "esno ./src/cli.ts",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.23.0",
    "@npmcli/config": "^8.3.4",
    "@types/kuler": "^2.0.2",
    "@types/lodash": "^4.17.7",
    "commander": "^12.1.0",
    "detect-indent": "^7.0.1",
    "fzf": "^0.5.2",
    "kuler": "^2.0.0",
    "lodash": "^4.17.21",
    "prompts": "^2.4.2",
    "strip-ansi": "^7.1.0",
    "zx": "^8.1.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.2",
    "@antfu/utils": "^0.7.10",
    "@aws-sdk/credential-provider-ini": "3.654.0",
    "@types/cli-progress": "^3.11.6",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.5.5",
    "@types/prompts": "^2.4.9",
    "bumpp": "^9.5.2",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.7",
    "deepmerge": "^4.3.1",
    "eslint": "^9.10.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "picocolors": "^1.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.1"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "off",
      "curly": "off"
    }
  }
}
